let testPosition = $('.test').position().top;
		let getScrollPosition = $(document).scrollTop();
		
		$(window).scroll(function(){
		 if (getScrollPosition > testPosition) {
			$('button').css('background-color', 'red');
		  }
		  else {
		  	$('button').css('background-color', 'green');
		  }
		});


